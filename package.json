{
  "name": "test-split-go",
  "version": "0.1.0",
  "private": "true",
  "description": "Splits go tests as an output that can be passed as the run argument to `go test` in a matrix",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/action.ts -o dist --license licenses.txt",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonc/go-test-split-action.git"
  },
  "author": {
    "name": "Brandon Croft",
    "email": "brandon.croft@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "@vercel/ncc": "^0.34.0",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
